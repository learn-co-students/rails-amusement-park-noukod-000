<div class='center'>
  <div class='info' >
    <h1 class="center">Login</h1>
<%= form_tag login_path do %>
  <p>Name: <%= select_tag "user_name",options_from_collection_for_select(@users,"name","name")%> </p><br>
  <p>Password:<%= password_field_tag :password %></p><br>
  <%= submit_tag("Sign In")%>
<%end%>

</div>
</div>
