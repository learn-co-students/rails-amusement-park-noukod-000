<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %> <br>

    <%= f.label :password_digest %>
    <%= f.password_field :password_digest %> <br>

    <%= f.label :happiness %> 
    <%= f.number_field :happiness %> <br>

    <%= f.label :tickets %> 
    <%= f.number_field :tickets %> <br>

    <%= f.label :height %> 
    <%= f.number_field :height %> <br>

    <%= f.label :nausea %> 
    <%= f.number_field :nausea %> <br>

    <%= f.submit  %>

<% end %>